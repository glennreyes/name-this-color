{
  "name": "name-this-color",
  "version": "1.0.0",
  "description": "Gives your colors a human-friendly name",
  "main": "server",
  "keywords": [
    "color",
    "name",
    "name-that-color",
    "human",
    "friendly",
    "readable"
  ],
  "author": "Glenn Reyes <office@glennreyes.com>",
  "homepage": "https://glennreyes.github.io/name-this-color",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/glennreyes/name-this-color.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf public",
    "build:static": "cross-env NODE_ENV=production webpack --config webpack.config.prod",
    "build": "npm run clean && npm run build:static",
    "coverage": "istanbul cover _mocha -- -R spec",
    "lint": "eslint .",
    "deploy": "gh-pages -d public",
    "start": "node server",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "travis": "npm run lint && npm test && npm run coverage"
  },
  "dependencies": {
    "draft-js": "^0.7.0",
    "highlight.js": "^9.3.0",
    "name-this-color": "git@github.com:glennreyes/name-this-color.git",
